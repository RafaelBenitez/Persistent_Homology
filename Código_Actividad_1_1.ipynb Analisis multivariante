      "cell_type": "code",
      "source": [
        "# Para la matriz de correlación, se obtiene la diagonal de la matriz de covarianzas, se eleva a la (-1/2)\n",
        "# se multiplica por la matriz de covarianzas y luego eso se multiplica nuevamente con la diagonal a la (-1/2)\n",
        "\n",
        "mat_d = pd.DataFrame(np.zeros((datos.shape[1], datos.shape[1])))\n",
        "\n",
        "np.fill_diagonal(mat_d.values, np.diagonal(mat_cov_var))\n",
        "\n",
        "mat_d12 = pd.DataFrame(np.zeros((datos.shape[1], datos.shape[1])))\n",
        "\n",
        "np.fill_diagonal(mat_d12.values, np.diagonal(mat_cov_var)**(-1/2))\n",
        "\n",
        "mat_r = np.dot(np.dot(mat_d12, mat_cov_var), mat_d12)\n",
        "mat_r = pd.DataFrame(mat_r, columns=datos.columns, index=datos.columns)\n",
        "mat_r"
      ],
