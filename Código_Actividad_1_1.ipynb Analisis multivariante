      "cell_type": "code",
      "source": [
        "# Para la matriz de varianzas, se obtiene la matriz de datos centrados, se transpone,\n",
        "# se multiplica con la original y se divide entre el número de datos\n",
        "\n",
        "mat_dat_cen = datos-vec_medias[\"Medias\"]\n",
        "\n",
        "mat_dat_cen_t = mat_dat_cen.transpose()\n",
        "\n",
        "mat_cov_var = np.dot(mat_dat_cen_t, mat_dat_cen)/datos.shape[0]\n",
        "mat_cov_var = pd.DataFrame(mat_cov_var, columns=datos.columns, index=datos.columns)\n",
        "mat_cov_var"
      ],
